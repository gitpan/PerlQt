#include <qpicture.h>

class QPicture : QPaintDevice {
    QPicture();
    virtual ~QPicture();
    const char * {bits($this->size())} data() const;
    bool isNull() const;
    bool load(const char *);
    bool play(QPainter *);
    bool save(const char *);
    void setData(const char * {bits *1}, uint {bitslen $1});
    uint size() const;
} Picture;
