#include <qvalidator.h>

suicidal virtual class QValidator : virtual QObject {
    QValidator(QWidget *, const char * = 0);
    virtual ~QValidator();
    virtual void fixup(QString &);
    abstract QValidator::State {enum} validate(QString &, int &);
} Validator;

%Valid = (
    Yes => QValidator::Acceptable,
    No => QValidator::Invalid,
    Maybe => QValidator::Valid,

    Invalid => QValidator::Invalid,
    Valid => QValidator::Valid,
    Acceptable => QValidator::Acceptable
);
